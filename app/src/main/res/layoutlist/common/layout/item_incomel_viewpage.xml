            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="4dp"
            android:text="薪资"
            android:textColor="@color/viewpage_item_text"
            android:textSize="@dimen/text_13" />
    </LinearLayout>

